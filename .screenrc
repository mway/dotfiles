shell -$SHELL
caption always "%{= kY}%-w%{= Yk}%n %t%{-}%+w%{ kG} %-= %LD %d %LM - %c"
altscreen on
maptimeout 5
defscrollback 5000

term screen-256color

attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce "on"

#backtick 1 15 15 rainbarf --bright --screen
#hardstatus string "%1`"
#hardstatus lastline

